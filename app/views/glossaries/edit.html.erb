<div class="crud_form">
  <h2>Edit Glossary</h2>

  <%= form_for @glossary do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <%= f.check_box :private %> Private Glossary?
    <%= f.submit "Update" %>
  <% end %>
</div>